{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "outDir": ".",
    "rootDir": "."
  },
  "include": ["preload.ts"],
  "exclude": ["node_modules", "src", "dist", "electron"]
}
